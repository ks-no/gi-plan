{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://no.ks.fiks.gi.plan.oppdatering/PlanvedtakMedUtsattRettsvirkning.v2.schema.json",
  "title": "PlanvedtakMedUtsattRettsvirkning",
  "description": "Kommunestyret har vedtatt planen med utsatt iverksetting pga. klage eller forventet klage.",

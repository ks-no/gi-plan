      "properties": {
        "posisjon": {
          "description": "Punkt ",
          "type": "object"
        },
        "dato": {
          "type": "string",
          "format": "date"
        },
        "saksnummer": {
          "type": "object",
          "properties": {
            "saks책r": {
              "type": "integer"
            },
            "sakssekvensnummer": {
              "type": "integer"
            }
          },
          "required": [
            "saks책r",
            "sakssekvensnummer"
          ]
        },
        "planbehandlingtype": {
          "type": "object",
          "properties": {
            "kodeverdi": {
              "type": "string"
            },
            "kodebeskrivelse": {
              "type": "string"
            }
          }
        },
        "navn": {
          "type": "string"
        }
      },
      "required": [
        "dato",
        "saksnummer",
        "planbehandlingtype",
        "navn"
      ]
